{
  "name": "@solarahealth/fhir-r4",
  "version": "1.0.0-alpha.2",
  "description": "TypeScript types and Zod schemas for HL7 FHIR R4 resources with runtime validation",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "private": false,
  "sideEffects": false,
  "keywords": [
    "fhir",
    "hl7",
    "healthcare",
    "interoperability",
    "typescript",
    "zod",
    "validation",
    "r4",
    "health-data"
  ],
  "author": "Solara Health Pty Ltd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SolaraHealthAU/fhir.git",
    "directory": "packages/fhir-r4"
  },
  "homepage": "https://github.com/SolaraHealthAU/fhir#readme",
  "bugs": {
    "url": "https://github.com/SolaraHealthAU/fhir/issues"
  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "rm -rf dist",
    "ci": "npm run typecheck && npm run lint && npm run test",
    "typecheck": "tsc --noEmit",
    "test": "NODE_ENV=test && jest",
    "lint": "eslint src",
    "generate:r4": "esr scripts/generate/R4/generate.ts",
    "generate": "./bin/download-specifications && npm run generate:r4"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/jest": "^29.5.12",
    "esbuild": "^0.25.1",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.9.0"
  },
  "gitHead": "aedb10dec75ecbd1824593eb938c1c5e2f9a64cd"
}
